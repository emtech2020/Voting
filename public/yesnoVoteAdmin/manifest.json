{
  "manifest_version": 2,
  "name": "yesnoVoteAdmin",
  "short_name": "yesnoVoteAdmin",
  "description": "",
  "version": "0.0.1",
  "minimum_chrome_version": "38",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },

  "content_security_policy": "script-src 'self' https://pwdemo.org; object-src 'self'",

  "app": {
    "background": {
      "scripts": [
        "background.js"
        ]
    }
  },
  
  "bluetooth": {
    "low_energy": true,
    "peripheral": true
  },
  
  "permissions": [
    "webview"
  ]
  
  

}
